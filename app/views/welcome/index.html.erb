<h1>Welcome to Delivery </h1>

<p style="color: red"><%= notice%> </p>

<% if current_user %>
  <p> Logged in as <strong><%= current_user.email %> </strong></p>
  <%= button_to "Sing out", destroy_user_session_path, method: :delete %>
<% else %>
  <%= link_to "Sing in", new_user_session_path %>
  or 
  <%= link_to "Sing out", new_user_registration_path %>
<%end%>